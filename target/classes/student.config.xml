<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean class="com.springCore.first.Student" name="student1">

      <!-- using setter injection by elment -->
      <property name="id">
         <value>1</value>
      </property>

      <property name="studentName">
         <value>Shivam</value>
      </property>

      <property name="studentAddress">
         <value>Husainpur</value>
      </property>

   </bean>

   <bean class="com.springCore.first.Student" name="student2">
      <!-- using setter injection using attributes -->
      <property name="id" value="2"/>
      <property name="studentName" value="amit"/>
      <property name="studentAddress" value="nagla man singh"/>
   </bean>

</beans>
